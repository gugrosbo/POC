{
  "version": "1.0.0-*",
  "compilationOptions": {
    "emitEntryPoint": true
  },
  "dependencies": {
    "WebProxy": "1.0.0"
  },
  "commands": {
    "web": "Microsoft.AspNet.Server.Kestrel"
  },
  "frameworks": {
    "dnx451": {
      "dependencies": {
        "Actor1": "1.0.0-*",
        "ContainerActor": "1.0.0-*",
        "ContainerActor.Interfaces": "1.0.0-*",
        "ImageActor": "1.0.0-*",
        "ImageActor.Interfaces": "1.0.0-*",
        "POC.HostActor.Interfaces": "1.0.0-*"
      },
      "frameworkAssemblies": {
        "System.Web": "4.0.0.0",
        "System.Web.Services": "4.0.0.0"
      }
    }
  },
  "exclude": [
    "wwwroot",
    "node_modules",
    "PackageRoot"
  ],
  "publishExclude": [
    "**.user",
    "**.vspscc",
    "PackageRoot"
  ],
  "entryPoint": "WebProxy",
  "loadable": false
}